{
  "name": "recents",
  "version": "1.4.0",
  "description": "View/open your recent projects on Atom's startup",
  "main": "./lib/main",
  "repository": "https://github.com/sht/atom-recents",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "standard"
  },
  "deserializers": {
    "RecentsView": "createView"
  },
  "consumedServices": {
    "metrics-reporter": {
      "versions": {
        "^1.1.0": "consumeReporter"
      }
    }
  },
  "configSchema": {
    "showOnStartup": {
      "type": "boolean",
      "default": true,
      "description": "Show recent projects tab when launching Atom."
    }
  },
  "dependencies": {
    "atom-project-util": "^4.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "etch": "0.14.0"
  },
  "devDependencies": {
    "standard": "^14.3.1"
  },
  "standard": {
    "globals": [
      "atom"
    ]
  }
}
